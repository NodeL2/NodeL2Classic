        "properties": {
            "classId": {
                "type": "integer"
            },
            "shortcuts": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "integer"
                        },
                        "slot": {
                            "type": "integer"
                        },
                        "kind": {
                            "type": "integer"
                        },
                        "level": {
                            "type": "integer"
                        }
                    },
                    "required": [
